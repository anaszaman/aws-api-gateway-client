{
  "name": "aws-api-gateway-client",
  "version": "0.2.2",
  "description": "Node.js moduel for AWS API Gateway client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kndt84/aws-api-gateway-client.git"
  },
  "keywords": [
    "aws",
    "apigateway"
  ],
  "author": "kndt84",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kndt84/aws-api-gateway-client/issues"
  },
  "homepage": "https://github.com/kndt84/aws-api-gateway-client#readme",
  "main": "dist/apigClient.js",
  "scripts": {
    "transpile": "babel src -d dist/",
    "prepublish": "npm run lint:js && npm run transpile",
    "lint": "node_modules/eslint/bin/eslint.js src lib *.js",
    "test": "npm run lint && nyc --reporter=text ava",
    "test:watch": "ava --watch"
  },
  "files": [
    "dist",
    "dist-es"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-es5-property-mutators"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "max-len": [
        "error",
        100
      ],
      "require-jsdoc": "off",
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "dependencies": {
    "axios": "^0.15.3",
    "crypto-js": "^3.1.8",
    "url": "^0.11.0",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es5-property-mutators": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.18.0",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "nyc": "^10.2.0"
  }
}
