{
  "name": "aws-api-gateway-client",
  "version": "0.2.17",
  "description": "A module for AWS API Gateway client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kndt84/aws-api-gateway-client.git"
  },
  "keywords": [
    "aws",
    "apigateway"
  ],
  "author": "kndt84",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kndt84/aws-api-gateway-client/issues"
  },
  "homepage": "https://github.com/kndt84/aws-api-gateway-client#readme",
  "main": "dist/apigClient.js",
  "scripts": {
    "transpile": "babel src -d dist/",
    "prepublish": "npm run lint && npm run transpile",
    "lint": "node node_modules/eslint/bin/eslint.js src",
    "test": "npm run lint && nyc --reporter=text ava",
    "test:watch": "ava --watch"
  },
  "files": [
    "dist",
    "dist-es"
  ],
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4",
      "es2017",
      "es2016",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-es5-property-mutators"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-retry": "^3.0.1",
    "crypto-js": "^3.1.9-1",
    "url": "^0.11.0",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "ava": "^0.25.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-es5-property-mutators": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^6.8.0",
    "nyc": "^11.1.0"
  }
}
